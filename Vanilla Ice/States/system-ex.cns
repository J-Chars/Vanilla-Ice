;This way of making a tandem is rather super extra, yes, but it's the only way that I know.
;Basically there's 75 variables taking in inputs. These inputs are read by the parent, which get's
;converted into it's own variable which is then read by the Stand to perform the necessary moves.
;This is also a more accurate way of creating a tandem where each individual input can be read.

[State 855, Reset]
type = VarRangeSet
trigger1 = root, stateno = 850
trigger1 = root, animelem = 1
first = 0
last = 59
value = 0

[State 855, Reset]
type = VarRangeSet
trigger1 = root, stateno = 850
trigger1 = root, animelem = 1
first = 0
last = 39
value = 0

[State 855, Buttons Pressed]
type = VarAdd
triggerall = Root, AILevel = 0
triggerall = (root, stateno = [850, 851])
triggerall = root, time >= 4
trigger1 = root, command = "x" || root, command = "y" || root, command = "z"
trigger2 = root, command = "a" || root, command = "b" || root, command = "c"
v = 0
value = 1

[State 855, Type of Button Pressed] ;stand punches
type = VarSet
triggerall = Root, AILevel = 0
triggerall = Var(0) != -1
triggerall = (root, stateno = [850, 851]) || root, numhelper(10000) > 0
triggerall = root, time >= 4 && root, command != "holddown"
trigger1 = root, command = "x" || root, command = "y" || root, command = "z"
fv = 39
value = cond(root, command = "x", 20300, cond(root, command = "y", 20310, 20320))

[State 855, Type of Button Pressed] ;stand kicks
type = VarSet
triggerall = Root, AILevel = 0
triggerall = (root, stateno = [850, 851]) || root, numhelper(10000) > 0
triggerall = root, time >= 4 && root, command != "holddown"
triggerall = Var(0) != -1
trigger1 = root, command = "a" || root, command = "b" || root, command = "c"
fv = 39
value = cond(root, command = "a", 20330, cond(root, command = "b", 20340, 20350))

